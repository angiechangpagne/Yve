{
  "compilerOptions":{
    "experimentalDecorators": true,
    "module": "commonjs",
    "target":"es2018",
    "lib": ["es6","esnext","es2015","es2018"],
    "suppressImplicitAnyIndexErrors": true,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "sourceMap": true,
    "inlineSources": true,
    "declaration": true,
    "importHelpers":true,
    "noImplicitAny": true,
    
  }
}